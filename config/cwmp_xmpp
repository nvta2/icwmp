	config cwmp 'xmpp'
	option enable '0'
	option id '0'
	option allowed_jid ''
	#Log levels: Critical=0, Warning=1, Notice=2, Info=3, Debug=4
	option loglevel '3'

config xmpp_connection
	option connection_instance '1'
	option enable '0'
	option connection_alias ''
	option username ''
	option password ''
	option domain 	''
	option resource ''
	option usetls '0'
	option interval '30'
	option attempt '16'
	option initial_retry_interval ''
	option retry_interval_multiplier ''
	option retry_max_interval ''
	option serveralgorithm 'DNS-SRV'
	
